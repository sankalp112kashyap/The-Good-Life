  <div class="col s12 ">
      <div id="inline-form" class="card card card-default scrollspy">
        <div class="card-content">

          <form method="post" action="/" class="col s12">

            <div class="row mt-0">


                <div class="input-field col m7 s12">
                  <!-- <i class="material-icons prefix">account_circle</i> -->
                  <input required name="wiseItem" id="icon_prefix1" type="text" class="validate">
                  <label for="icon_prefix1" class="">What did you learn today?</label>
                </div>

                <div class="input-field col m4 s10">
                  <!-- <i class="material-icons prefix">lock_outline</i> -->
                  <input required name="wiseCategory" id="icon_password" type="text" class="validate">
                  <label for="icon_password" class="">Category</label>
                </div>

                <div class="input-field col m1 s2">
                  <div class="input-field col s2">
                    <button onclick="setHourID();"
                    class="waves-effect waves-circle waves-light btn-floating btn-medium  gradient-45deg-light-blue-cyan"
                    type="submit"> <!-- secondary content -->
                      <i class="material-icons">add</i></button>
                  </div>
                </div>

                <input name="hour" id="hour" type="hidden" value="">



             </div>
          </form>

          <!-- <div class="ct-chart ct-perfect-fourth"></div> -->


          <div id="view-higlight-table" class="active">
              <div class="row">
                    <div class="col s12">

                      <table class="highlight">
                      <!--   <thead>
                          <th>Hour</th>
                          <th>Activity</th>
                          <th>Category</th>
                        </thead>
-->                          <tbody>

                          <% wiseListItems.forEach(function(item) { %>
                          <tr>
                            <td> <i> <span class="hour"> <%=item.hour%>-<%=item.hour+1%> </span> </i> </td>
                            <td> <%=item.description%> </td>
                            <td> <%=item.category%> </td>
                          </tr>
                          <% }) %>

                      </table>

                    </div>
              </div>
          </div>



        </div>
      </div>
    </div>







<div class="row mt-4">
   <div class="col s12 m4 l4">


      <!-- Current Balance -->

      <div class="card animate fadeLeft">
         <div class="card-content">
            <h4 class="card-title mb-0">Categories Of Wisdom  <i class="material-icons float-right"></i></h4>
            <p class="medium-small">In the last 28 days</p>
            <div class="current-balance-container">
               <div id="current-balance-donut-chart" class="current-balance-shadow"></div>
            </div>
            <% if ( typeof(efforts_tot) !== "undefined" && efforts_tot.length > 0) { %>
               <h5 class="center-align"><%=efforts_tot[0].hours%> hr</h5>
            <% } %>
            <p class="medium-small center-align">Worked In the last 28 days</p>
         </div>
      </div>
   </div>



   <div class="col s12 m8 l8 animate fadeRight">
      <!-- Total Transaction -->
      <div class="card">
         <div class="card-content">
            <h4 class="card-title mb-0">Number of Hours Worked <i class="material-icons float-right"></i></h4>
            <p class="medium-small">In the last week</p>
            <div class="total-transaction-container">
               <div id="total-transaction-line-chart" class="total-transaction-shadow"></div>
            </div>
         </div>
      </div>
   </div>
</div>
<!--/ Current balance & total transactions cards-->




<!-- User statistics & appointment cards-->
<div class="row">
   <div class="col s12 l5">
      <!-- User Statistics -->
      <div class="card user-statistics-card animate fadeLeft">
         <div class="card-content">
            <h4 class="card-title mb-0">User Statistics<i class="material-icons float-right"></i></h4>
            <div class="row">
               <div class="col s12 m6">
                  <ul class="collection border-none mb-0">
                     <li class="collection-item avatar">
                        <i class="material-icons circle pink accent-2">trending_up</i>
                        <p class="medium-small">Happiness</p>
                        <h5 class="mt-0 mb-0">60%</h5>
                     </li>
                  </ul>
               </div>
               <div class="col s12 m6">
                  <ul class="collection border-none mb-0">
                     <li class="collection-item avatar">
                        <i class="material-icons circle purple accent-4">trending_down</i>
                        <p class="medium-small">Anxiety</p>
                        <h5 class="mt-0 mb-0">40%</h5>
                     </li>
                  </ul>
               </div>
            </div>
            <div class="user-statistics-container">
               <div id="user-statistics-bar-chart" class="user-statistics-shadow"></div>
            </div>
         </div>
      </div>
   </div>
   <div class="col s12 l4">
      <!-- Recent Buyers -->
      <div class="card recent-buyers-card animate fadeUp">
         <div class="card-content">
            <h4 class="card-title mb-0">Leaderboard<i class="material-icons float-right"></i></h4>
            <p class="medium-small pt-2">Wisdom</p>
            <ul class="collection mb-0">
               <li class="collection-item avatar">
                  <img src="/images/avatar/avatar-7.png" alt="" class="circle" />
                  <p class="font-weight-600">Rishabh Jaiswal</p>
                  <p class="medium-small">Rank 1</p>
                  <!-- <a href="#!" class="secondary-content"><i class="material-icons">star_border</i></a> -->
               </li>
               <li class="collection-item avatar">
                  <img src="/images/avatar/avatar-3.png" alt="" class="circle" />
                  <p class="font-weight-600">Ayush Srivastava</p>
                  <p class="medium-small">Rank 2</p>
                  <!-- <a href="#!" class="secondary-content"><i class="material-icons">star_border</i></a> -->
               </li>
               <li class="collection-item avatar">
                  <img src="/images/avatar/avatar-5.png" alt="" class="circle" />
                  <p class="font-weight-600">Raloue Kapoor</p>
                  <p class="medium-small">Rank 3</p>
                  <!-- <a href="#!" class="secondary-content"><i class="material-icons">star_border</i></a> -->
               </li>
            </ul>
         </div>
      </div>
   </div>
   <div class="col s12 l3">
      <div class="card animate fadeRight">
         <div class="card-content">
            <h4 class="card-title mb-0">User Survey Results</h4>
            <div class="conversion-ration-container mt-8">
               <div id="conversion-ration-bar-chart" class="conversion-ration-shadow"></div>
            </div>
            <p class="medium-small center-align"> Productivity Increased By</p>
            <h5 class="center-align mb-0 mt-0">82%</h5>
         </div>
      </div>
   </div>
</div>
